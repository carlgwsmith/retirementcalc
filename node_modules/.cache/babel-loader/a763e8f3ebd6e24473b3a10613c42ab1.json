A;AAQA,QAAA,kBAAA,EAAA;AARA;AAXA,KAAA;AAsBA,GA7BA;AA8BA,EAAA,KAAA,EAAA;AACA,IAAA,IAAA,EAAA;AACA,MAAA,OAAA,EAAA,iBAAA,QAAA,EAAA;AACA,aAAA,YAAA,CAAA,eAAA,GAAA,QAAA,CAAA,QAAA,CAAA,QAAA,CAAA,GAAA,QAAA,CAAA,QAAA,CAAA,SAAA,CAAA;AACA,aAAA,YAAA,CAAA,cAAA,GAAA,QAAA,CAAA,QAAA,CAAA,SAAA,CAAA,GAAA,QAAA,CAAA,QAAA,CAAA,UAAA,CAAA;AACA,aAAA,YAAA,CAAA,SAAA,GAAA,QAAA,CAAA,QAAA,CAAA,YAAA,CAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,cAAA,CAAA;AACA,aAAA,YAAA,CAAA,iBAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,SAAA,CAAA,GAAA,QAAA,CAAA,QAAA,CAAA,YAAA,CAAA;AACA,aAAA,YAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,QAAA,CAAA,gBAAA,CAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,eAAA,CAAA;AACA,aAAA,YAAA,CAAA,qBAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,iBAAA,CAAA,GAAA,QAAA,CAAA,QAAA,CAAA,gBAAA,CAAA,CANA,CAOA;;AACA,aAAA,YAAA,CAAA,kBAAA,GAAA,QAAA,CAAA,QAAA,CAAA,cAAA,CAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,eAAA,CAAA;AACA,aAAA,YAAA,CAAA,eAAA,GAAA,QAAA,CAAA,KAAA,YAAA,CAAA,qBAAA,CAAA,GAAA,QAAA,CAAA,QAAA,CAAA,cAAA,CAAA;AACA,OAXA;AAYA,MAAA,IAAA,EAAA;AAZA;AADA,GA9BA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,OAAA,GAAA;AACA,QAAA,SAAA,EAAA,KAAA,IAAA,CAAA,SADA;AAEA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QAFA;AAGA,QAAA,eAAA,EAAA,KAAA,YAAA,CAAA,eAHA;AAIA,QAAA,cAAA,EAAA,KAAA,YAAA,CAAA,cAJA;AAKA,QAAA,SAAA,EAAA,KAAA,YAAA,CAAA,SALA;AAMA,QAAA,iBAAA,EAAA,KAAA,YAAA,CAAA,iBANA;AAOA,QAAA,gBAAA,EAAA,KAAA,YAAA,CAAA,gBAPA;AAQA,QAAA,qBAAA,EAAA,KAAA,YAAA,CAAA,qBARA;AASA,QAAA,eAAA,EAAA,KAAA,YAAA,CAAA,eATA;AAUA,QAAA,kBAAA,EAAA,KAAA,YAAA,CAAA,kBAVA;AAWA,QAAA,gBAAA,EAAA,KAAA,IAAA,CAAA;AAXA,OAAA,CADA,CAeA;AACA;;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,UAAA,EAAA,OAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AACA;AApBA;AA9CA,CAAA","sourcesContent":["<template>\n  <div class=\"formContainer\">\n     <form method=\"post\" action=\"#\">\n       <form-wizard color=\"#0de4b5\" @on-complete=\"emitData\">\n         <h5 slot=\"title\">Fill out a few simple questions and we'll give you a rundown of your retirement expectations.</h5>   \n        <tab-content title=\"Personal details\">\n          <input-field unique=\"currentage\" label=\"Current Age\" v-on:updated=\"form.currentage = $event\"></input-field>\n          <input-field unique=\"retireage\" label=\"Retirement Age\" v-on:updated=\"form.retireage = $event\"></input-field>\n          <input-field unique=\"lifespan\" label=\"Expected Lifespan\" v-on:updated=\"form.lifespan = $event\"></input-field>\n        </tab-content>\n        <tab-content title=\"Additional Info\">\n          <input-field unique=\"totalsavings\" label=\"Current Total Savings\" v-on:updated=\"form.totalsavings = $event\"></input-field>\n          <input-field unique=\"annualincome\" label=\"Annual Household Income\" v-on:updated=\"form.annualincome = $event\"></input-field>\n        </tab-content>\n        <tab-content title=\"Last step\">\n          <input-field unique=\"annualspending\" label=\"What is your expected annual spending in retirement?\" v-on:updated=\"form.annualspending = $event\"></input-field>\n          <input-field unique=\"retirementsalary\" label=\"Expected Annual Income in Retirement\" v-on:updated=\"form.retirementsalary = $event\"></input-field>\n        </tab-content>\n        </form-wizard>\n            <!-- <div class=\"input-wrapper\">\n                <b-button block @click.prevent=\"emitData\">Calculate</b-button>\n            </div> -->\n      </form>\n  </div>\n</template>\n\n<script>\nimport InputField from '../components/InputField.vue'\nimport {FormWizard, TabContent} from 'vue-form-wizard'\nimport 'vue-form-wizard/dist/vue-form-wizard.min.css'\n// import EventBus from '../eventBus'\n\nexport default {\n  components: {\n    InputField,\n    FormWizard,\n    TabContent\n  },\n  data() {\n    return {\n        form: {\n          currentage:0,\n          retireage: 0,\n          annualincome: 0,\n          yearlyinterest: 0,\n          totalsavings:0,\n          annualspending: 0,\n          retirementsalary: 0,\n          lifespan: 0\n        },\n        returnedData:{\n        retirementyears: 0,\n        yearstilretire:0,\n        moneyatretirement: 0,\n        moneymade:0,\n        incomelongevity: 0,\n        retirementincome: 0,\n        retirementwithsavings: 0,\n        retirementspending: 0\n        }\n    }\n  },\nwatch: {\n    form: {\n      handler: function(newValue) {\n        this.returnedData.retirementyears = parseInt(newValue.lifespan) - parseInt(newValue.retireage);\n        this.returnedData.yearstilretire = parseInt(newValue.retireage) - parseInt(newValue.currentage);\n        this.returnedData.moneymade = parseInt(newValue.annualincome) * parseInt(this.returnedData.yearstilretire);\n        this.returnedData.moneyatretirement = parseInt(this.returnedData.moneymade) + parseInt(newValue.totalsavings);\n        this.returnedData.retirementincome = parseInt(newValue.retirementsalary) * parseInt(this.returnedData.retirementyears);\n        this.returnedData.retirementwithsavings = parseInt(this.returnedData.moneyatretirement) + parseInt(newValue.retirementsalary);\n        //this.returnedData.retirementspending= parseInt(this.returnedData.retirementwithsavings) - (parseInt(newValue.annualspending) * parseInt(this.returnedData.retirementyears));\n        this.returnedData.retirementspending= parseInt(newValue.annualspending) * parseInt(this.returnedData.retirementyears);\n        this.returnedData.incomelongevity = parseInt(this.returnedData.retirementwithsavings) / parseInt(newValue.annualspending); \n      },\n      deep: true\n    }\n  },\n  methods: {\n    emitData () {\n      const payload = {\n        retireage: this.form.retireage,\n        lifespan: this.form.lifespan,\n        retirementyears: this.returnedData.retirementyears,\n        yearstilretire: this.returnedData.yearstilretire,\n        moneymade: this.returnedData.moneymade,\n        moneyatretirement: this.returnedData.moneyatretirement,\n        retirementincome: this.returnedData.retirementincome,\n        retirementwithsavings: this.returnedData.retirementwithsavings,\n        incomelongevity: this.returnedData.incomelongevity,\n        retirementspending: this.returnedData.retirementspending,\n        retirementsalary: this.form.retirementsalary\n      }\n\n      // EventBus.$emit(\"returnData\", this.returnedData.yearstilretire);\n      // console.log('emit!');\n      this.$store.commit('setState', payload);\n      this.$router.push('/Results');\n    }\n  }\n}\n</script>\n\n<style scoped>\n.formContainer{\nborder: 6px solid rgb(230, 230, 230);\npadding: 20px;\nborder-radius: 15px;\nbox-shadow: 2px 2px 2px rgb(240, 240, 240);\nbackground: white;\n}\ninput {\n    margin-bottom: 16px !important;\n}\n.vue-form-wizard .wizard-icon-circle {\n    border: 5px solid #0de4b5;\n    background-color:#0de4b5;\n}\n.finish-button{\n  background-color:#43A047 !important;\n  border-color: #43A047 !important;\n}\n</style>"],"sourceRoot":"src/components"}]}